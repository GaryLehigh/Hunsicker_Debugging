        function submitEngine(){
            
            var year = $("#Year").val();
            if(year ==-1){
                alert("Please select a Year and then click next.");
                event.preventDefault();
                return;

            }
            
        
            
            //window.location = "/chooseExistingContactBasedOnCompanyID/"+CompanyID;

        }
        function back(){
            var link1= '/chooseExistingEngineBasedOnVehicle?VIN='+$("#VIN").val()+"&ContactID="+$("#ContactID").html()+"&CompanyID="+$("#CompanyID").html()+"&SerialNumber="+$("#SerialNumber_temp").html();
            location.href=link1;
        }
        function BackToJobConclusion(){
            var aaa =$("#back_URL").html();
            var elem = document.createElement('textarea');
            elem.innerHTML = aaa;
            var decoded = elem.value;
            //bbb  = aaa.replace("/amp;", "&");
            
            //alert(decoded);
            location.href = decoded;
            
        }
        
